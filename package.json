{
  "name": "vendorvert",
  "version": "0.0.7",
  "description": "Sistema de ayuda a la toma de decisiones para pequeños negocios.",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsc --build",
    "start": "node dist/index.js",
    "clean": "tsc --build --clean",
    "test": "jest",
    "coverage": "jest --coverage --no-cache",
    "postclean": "rm -rf dist && rm -rf docs/coverage",
    "docker:build": "docker build -t vendorvert .",
    "docker:run:local": "docker run -e LOG_DIR=`pwd`/logs -e LOG_FILE=`logs.log` -t -v `pwd`:/app/test vendorvert",
    "docker:run:ci": "docker run -e LOG_DIR=$LOG_DIR -e LOG_FILE=$LOG_FILE -t -v `pwd`:/app/test vendorvert"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/modejota/vendorvert.git"
  },
  "keywords": [
    "cloud",
    "computing",
    "ugr",
    "master"
  ],
  "author": {
    "name": "José Alberto Gómez García",
    "email": "modej@correo.ugr.es"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/modejota/vendorvert/issues",
    "email": "modej@correo.ugr.es"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "etcd3": "^1.1.0",
    "fastify": "^4.10.0",
    "pino": "^8.7.0"
  }
}
